            "cmd-d": "menu::SelectNext",
            "cmd-j": "editor::ScrollCursorCenter",
            "cmd-h": "editor::Hover",
            "cmd-l": "go_to_line::Toggle",
            "cmd-o": "outline::Toggle",
            "alt-up": "editor::MoveLineUp",
            "alt-down": "editor::MoveLineDown",
            "editor::CutToEndOfLine": "editor::DeleteLine",
            "ctrl-alt-up": "editor::SelectLargerSyntaxNode",
            "ctrl-alt-down": "editor::SelectSmallerSyntaxNode",
            "ctrl-alt-right": "pane::SplitRight",
            "cmd-/": "editor::ToggleComments",
            "alt-ctrl-h": "editor::Hover",
        }
    },
